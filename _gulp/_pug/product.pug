doctype

html(lang="ru")

  -var active = 'Кресло MARQUETTE' //активный пункт меню, список в header.jade
  -var pageTitle = 'Кресло MARQUETTE' + ' - Эксклюзивный магазин MAISON' //title страницы для генерации верстки
  -var lk = false

  include ./_include/_head

  body
    .base
      // Preloader
      include ./_include/_preloader

      include ./_include/_header

      // Top img
      .top-img.--thin
        img(src="./tmp/top-img-1.png", alt="").top-img__bg
        .top-img__wrapper
          .container

            .h1 Элитные диваны
            +breadcrumbs()
              +breadcrumbs-item('Эксклюзивный магазин MAISON', './index.html')
              +breadcrumbs-item('Название раздела', '#!')
              +breadcrumbs-item('Название страницы')

      // Content
      .content
        .container

          // Product
          .product
            .row
              .col-12.col-lg-6
                .product__head.--top
                  .div1
                    h1 Кресло MARQUETTE
                    .product__sku SKU: D-12525
                  .div2
                    a(href="#!").product__brand
                      img(src="./tmp/main-choice-logo-4.png", width="95", height="95", alt="")
                      .span
                .product__gallery
                  .product__gallery-top
                    .product__carousel-big
                      a(href="./tmp/product-gallery-1.png" data-fancybox="images").product__gallery-block
                        img(src="./tmp/product-gallery-1.png", width="346", height="346" alt="")
                      a(href="./tmp/product-gallery-2.png" data-fancybox="images").product__gallery-block
                        img(src="./tmp/product-gallery-2.png", width="346", height="346" alt="")
                      a(href="./tmp/product-gallery-3.png" data-fancybox="images").product__gallery-block
                        img(src="./tmp/product-gallery-3.png", width="346", height="346" alt="")
                      a(href="./tmp/product-gallery-4.png" data-fancybox="images").product__gallery-block
                        img(src="./tmp/product-gallery-4.png", width="346", height="346" alt="")
                      a(href="./tmp/product-gallery-5.png" data-fancybox="images").product__gallery-block
                        img(src="./tmp/product-gallery-5.png", width="346", height="346" alt="")
                  .product__gallery-bottom
                    .product__carousel-small
                      img(src="./tmp/product-gallery-1.png", width="70", height="70" alt="")
                      img(src="./tmp/product-gallery-2.png", width="70", height="70" alt="")
                      img(src="./tmp/product-gallery-3.png", width="70", height="70" alt="")
                      img(src="./tmp/product-gallery-4.png", width="70", height="70" alt="")
                      img(src="./tmp/product-gallery-5.png", width="70", height="70" alt="")

              .col-12.col-lg-6
                .product__head.--bottom
                  .div1
                    .h1 Кресло MARQUETTE
                    .product__sku SKU: D-12525
                  .div2
                    a(href="#!").product__brand
                      img(src="./tmp/main-choice-logo-4.png", width="95", height="95", alt="")
                      .span
                .product__description
                  h3 Описание
                  p Один из самых выдающихся современных дизайнеров, обладатель титула Командора Британской империи, американец Ральф Лорен более 30 лет производит предметы интерьера под торговой маркой Ralph Lauren Home. О своей мебели сам дизайнер когда-то ёмко сказал: "Я продаю мечту". 
          // /end Product

