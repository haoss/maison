                        img(src="./tmp/product-gallery-3.png", width="346", height="346" alt="")
                      a(href="./tmp/product-gallery-4.png" data-fancybox="images").product__gallery-block
                        img(src="./tmp/product-gallery-4.png", width="346", height="346" alt="")
                      a(href="./tmp/product-gallery-5.png" data-fancybox="images").product__gallery-block
                        img(src="./tmp/product-gallery-5.png", width="346", height="346" alt="")
                  .product__gallery-bottom
                    .product__carousel-small
                      img(src="./tmp/product-gallery-1.png", width="70", height="70" alt="")
                      img(src="./tmp/product-gallery-2.png", width="70", height="70" alt="")
                      img(src="./tmp/product-gallery-3.png", width="70", height="70" alt="")
                      img(src="./tmp/product-gallery-4.png", width="70", height="70" alt="")
                      img(src="./tmp/product-gallery-5.png", width="70", height="70" alt="")

              .col-12.col-lg-6
                .product__head.--bottom
                  .div1
                    .h1 Кресло MARQUETTE
                    .product__sku SKU: D-12525
                  .div2
                    a(href="#!").product__brand
                      img(src="./tmp/main-choice-logo-4.png", width="95", height="95", alt="")
                      .span
                .product__price
                  strong 99 199
                  i.icon-rouble
                .product__availability
