                    li: a(href="#!")
                      img(src="./tmp/cart-small-img.png", width="50", height="50", alt="")
                .product__th
                  h3 Характеристики
                  table
                    tbody
                      tr
                        td Производитель
                        td Novelty
                      tr
                        td Ширина, мм
                        td 1500
                      tr
                        td Высота, мм
                        td 1200
                      tr
                        td Длина, мм
                        td 2500
