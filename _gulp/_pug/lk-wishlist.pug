doctype

html(lang="ru")

  -var active = 'Список желаний' //активный пункт меню, список в header.jade
  -var pageTitle = 'Список желаний' + ' - Эксклюзивный магазин MAISON' //title страницы для генерации верстки

  include ./_include/_head

  body
    .base
      // Preloader
      include ./_include/_preloader

      include ./_include/_header

      // Content
      .content
        .container

          .row
            .col-12.col-lg-4.col-xl-3
              nav.lk-navigation
                ul
                  li: a(href="#!") Профиль
                  li: a(href="#!") История заказов
                  li: a(href="#!") Список желаний
                  li.last: a(href="#!") Выйти с сайта
            .col-12.col-lg-8.col-xl-9
              .lk-head
                .div1
                  h1 Список желаний
                .div2

      // /end Content

      include ./_include/_footer
      //- div(style="height: 1000px; background-color: #000")

    // Load Scripts
    include ./_include/_footer-js
    script(src="./js/common.js")

